{
    "name": "topix-hackademy/laravel-contact-tool-sdk",
    "type": "library",
    "description": "Laravel Set of Models to perform access to Contact and Company data models.",
    "keywords": ["Topix", "Top-ix", "ContactToolSdk", "Laravel", "Hackademy"],
    "homepage": "https://github.com/topix-hackademy/laravel-contact-tool-sdk",
    "license": "MIT",
    "authors": [
        {
            "name": "massimo_santoli",
            "email": "santoli.massimo@gmail.com",
            "homepage": "http://www.top-ix.org/it/home/",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.2",
        "php" : "~5.5|~7.0",
        "guzzlehttp/guzzle":"~6.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3",
        "laravel/framework": "~5.2"
    },
    "autoload": {
        "psr-4": {
            "Topix\\Hackademy\\ContactToolSdk\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Topix\\Hackademy\\ContactToolSdk\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "format": "phpcbf --standard=psr2 src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "sprint-1"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
